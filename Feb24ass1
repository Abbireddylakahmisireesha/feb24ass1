def isprime(n): 
  if n==1:
    return False
  for i in range(2,n):
    if n%i==0:
      return False
  return True
n=int(input('enter value:'))
I=[]
c=0
for i in range(n):
  s=int(input())
    I.append(s) 
print(I)
